<h1>{{title}}</h1>
<!--list product table-->
<table>
    <thead>
        <tr>
            <th>Product Name</th>
            <th>Price</th>
            <th>Category</th>
            <th>Image</th>
        </tr>
    </thead>
    <tbody>
        {{#each products}}
        <tr>
            <td>{{name}}</td>
            <td>{{price}}</td>
            <td>{{category}}</td>
            <td><img src="{{image}}" alt="{{name}}" width="100" height="100"></td>
        </tr>
        {{/each}}
    </tbody>
</table>